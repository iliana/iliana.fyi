{
  "scripts": {
    "build": "webpack && npm run postcss && zola build && util/subset-fonts.sh",
    "dev": "util/subset-fonts.sh dev && concurrently --kill-others npm:watch-webpack npm:watch-postcss npm:serve",
    "fix": "prettier -w .",
    "postcss": "postcss styles.css -o static/styles.css",
    "serve": "zola serve --drafts",
    "watch-postcss": "npm run postcss && npm run postcss -- -w",
    "watch-webpack": "webpack --watch"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@tailwindcss/typography": "^0.5.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
    "cssnano": "^5.1.7",
    "eval": "^0.1.8",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "tailwindcss": "^3.0.23",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "printWidth": 120,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "ignore",
    "importOrder": [
      "^[./]"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  }
}
