{
  "scripts": {
    "build": "fonts/fetch.sh && webpack --node-env production && zola build && fonts/subset.sh",
    "dev": "fonts/fetch.sh && concurrently --kill-others \"zola serve --drafts\" \"webpack --watch\"",
    "fix": "prettier -w . && eslint --ext .js,.jsx --fix ."
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/babel-preset": "^6.2.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "concurrently": "^7.0.0",
    "css-loader": "^6.7.1",
    "css.gg": "^2.0.0",
    "cssnano": "^5.1.7",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eval": "^0.1.8",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "printWidth": 120,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "ignore"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "public/",
      "static/"
    ],
    "parser": "@babel/eslint-parser",
    "plugins": [
      "@babel"
    ],
    "extends": [
      "airbnb",
      "prettier"
    ],
    "rules": {
      "import/order": [
        "error",
        {
          "newlines-between": "never",
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "no-multi-assign": [
        "error",
        {
          "ignoreNonDeclaration": true
        }
      ],
      "react/prop-types": "off"
    }
  },
  "browserslist": [
    "defaults",
    "not ie 11"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
